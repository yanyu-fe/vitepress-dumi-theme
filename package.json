    "shiki": "^0.12.1"
  },
  "devDependencies": {
    "@mistjs/eslint-config-vue": "^0.0.3",
    "@mistjs/tsconfig-vue": "^0.0.3",
    "cpx": "^1.5.0",
    "eslint": "^8.31.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2",
    "vite": "^4.0.4",
    "vitepress": "1.0.0-alpha.35",
    "vitest": "^0.26.3",
    "vue": "^3.2.45"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
