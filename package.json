{
  "name": "vitepress-dumi-theme",
  "type": "module",
  "version": "0.0.1-beta",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "main": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/node/index.js"
    },
    "./theme": {
      "import": "./dist/theme/index.js"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "theme"
