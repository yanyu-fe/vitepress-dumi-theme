  "scripts": {
    "fix": "eslint src --fix",
    "docs:dev": "vitepress dev docs",
    "build": "run-s build:source cp",
    "build:source": "unbuild",
    "cp": "cpx \"dist/theme/index.d.ts\" theme"
  },
  "dependencies": {
    "@docsearch/css": "^3.3.1",
    "@docsearch/js": "^3.3.1",
    "@vueuse/core": "^9.10.0",
